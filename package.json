{
  "name": "vue-level",
  "version": "1.0.0",
  "description": "A wrapper of level-js for Vue.",
  "main": "main.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name vue-level ./src/main.js && rimraf ./dist/demo.html"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "level-js": "^5.0.2",
    "levelup": "^4.4.0"
  },
  "devDependencies": {
    "rimraf": "^2.7.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
